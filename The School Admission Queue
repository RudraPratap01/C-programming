#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

struct Student{
    char name[50];
    int age;
    char grade;
};
struct Student s;
int main() {
    int n;
    scanf("%d ",&n);
    for(int i=1;i<=n;i++){
    gets(s.name);//scanf("%s",s.name);
    scanf("%d ",&s.age);
    scanf("%c ",&s.grade);
    printf("Student %d:\nName: %s\nAge: %d\nGrade: %c\n",i,s.name,s.age,s.grade);
    }
    return 0;
}
